{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "value": "__plan01.name__"
        },
        "location": {
            "value": "__location__"
        },
        "tags": {
            "value": {
              "environment": "__tags.environment__",
              "role": "__tags.role__",
              "charge-to": "__tags.charge-to__",
              "application": "__tags.application__"
            }
          },
              "sku": {
            "value": "__plan01.sku.name__"
        },
        "skucode": {
            "value": "__plan01.sku.code__"
        },
        "workerSize": {
            "value": "__plan01.worker.size__"
        },
        "workerSizeId": {
            "value": "__plan01.worker.sizeId__"
        },
        "numberOfWorkers": {
            "value": "__plan01.worker.number__"
        }
    }
}